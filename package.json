{
  "name": "txtracer",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "homepage": "https://txtracer.ton.org/",
  "scripts": {
    "dev": "vite --port 5174",
    "build": "tsc -b && vite build",
    "build:all": "yarn build && yarn astro:build",
    "lint": "eslint --max-warnings 0 . --cache",
    "preview": "vite preview",
    "deploy": "gh-pages -d dist --nojekyll",
    "fmt": "prettier --write -l --cache .",
    "fmt:check": "prettier --check --cache .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "yarn playwright test",
    "coverage": "vitest run --coverage",
    "precommit": "yarn fmt && yarn lint --fix && yarn build",
    "spell": "cspell --no-progress spec/src/content/docs",
    "astro:dev": "cd spec && astro dev",
    "astro:clean": "rm -rf dist/spec/doc out/ .astro/",
    "astro:build": "yarn astro:clean && cd spec && yarn spell && astro check && astro build"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.7.0",
    "@ton-community/func-js": "^0.9.1",
    "@ton/core": "^0.61.0",
    "@ton/crypto": "^3.3.0",
    "@ton/sandbox": "^0.36.0",
    "@ton/test-utils": "^0.7.0",
    "@ton/tolk-js": "^1.0.0",
    "@truecarry/tlb-abi": "^0.2.0",
    "@xyflow/react": "^12.3.2",
    "allotment": "^1.20.4",
    "buffer": "^6.0.3",
    "cspell": "^9.2.1",
    "framer-motion": "^12.11.0",
    "monaco-editor": "^0.52.2",
    "react": "^19.1.0",
    "react-d3-tree": "^3.6.6",
    "react-dom": "^19.1.0",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "ton-assembly": "0.2.4",
    "txtracer-core": "^0.2.0"
  },
  "devDependencies": {
    "@astrojs/check": "0.9.4",
    "@astrojs/markdown-remark": "5.3.0",
    "@astrojs/react": "^4.3.0",
    "@astrojs/starlight": "0.29.3",
    "@eslint/js": "^9.25.0",
    "@playwright/test": "^1.52.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/d3": "^7.4.3",
    "@types/node": "^22.15.17",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/ws": "^8.5.10",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/ui": "^3.1.3",
    "astro": "4.16.19",
    "eslint": "^9.25.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.3.4",
    "eslint-plugin-functional": "^9.0.1",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-unused-imports": "^4.1.4",
    "gh-pages": "^6.3.0",
    "globals": "^16.0.0",
    "jsdom": "^26.1.0",
    "playwright": "^1.52.0",
    "prettier": "3.4.2",
    "rehype-autolink-headings": "7.1.0",
    "remark": "^15.0.1",
    "remark-custom-heading-id": "2.0.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "sharp": "0.32.6",
    "starlight-links-validator": "^0.12.1",
    "starlight-llms-txt": "^0.5.0",
    "starlight-theme-rapide": "^0.2.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vitest": "^3.1.3",
    "ws": "^8.15.0"
  },
  "packageManager": "yarn@4.6.0",
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": false,
    "printWidth": 100,
    "semi": false,
    "singleQuote": false,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false,
    "overrides": [
      {
        "files": [
          "*.yaml",
          "*.yml"
        ],
        "options": {
          "tabWidth": 2
        }
      }
    ]
  }
}
